        local br, onScreenBR = Camera:WorldToViewportPoint(bottomRight)

        if not (onScreenTL and onScreenTR and onScreenBL and onScreenBR) then
            box.Visible = false
            return
        end

        if not (isValidVector(tl) and isValidVector(tr) and isValidVector(bl) and isValidVector(br)) then
            box.Visible = false
            return
        end

        box.PointA = Vector2.new(tl.X, tl.Y)
        box.PointB = Vector2.new(tr.X, tr.Y)
        box.PointC = Vector2.new(br.X, br.Y)
        box.PointD = Vector2.new(bl.X, bl.Y)
        box.Visible = true
    end)

    espObjects[player] = {
        box = box,
        tag = tag,
        connection = connection
    }
end

local function refreshESP()
    cleanupESP()
    for _, player in pairs(Players:GetPlayers()) do
        createESP(player)
    end
end

Players.PlayerAdded:Connect(function(player)
    player.CharacterAdded:Connect(function()
        task.wait(0.05)
        refreshESP()
    end)
end)

Players.PlayerRemoving:Connect(function(player)
    if espObjects[player] then
        cleanupESP()
    end
end)

task.spawn(function()
    while true do
        task.wait(0.05)
        refreshESP()
    end
end)
end)

modtab:Button("Mod Gun (ALL MODS)", function()
local repstorage = game.ReplicatedStorage

for i, v in pairs(repstorage.Weapons:GetDescendants()) do
   if v.Name == "Auto" then
       v.Value = true
   end
   if v.Name == "RecoilControl" then
       v.Value = 0
   end
   if v.Name == "MaxSpread" then
       v.Value = 0
   end
   if v.Name == "ReloadTime" then
      v.Value = 0.1
   end
   if v.Name == "FireRate" then
       v.Value = 0.05
   end
end
end)

local repstorage = game.ReplicatedStorage
local weapons = repstorage:WaitForChild("Weapons")

function enableauto()
    for _, v in pairs(weapons:GetDescendants()) do
        if v.Name == "Auto" then
            v.Value = true
        end
    end
end

function norecoil()
    for _, v in pairs(weapons:GetDescendants()) do
        if v.Name == "RecoilControl" then
            v.Value = 0
        end
    end
end

function nospread()
    for _, v in pairs(weapons:GetDescendants()) do
        if v.Name == "MaxSpread" then
            v.Value = 0
        end
    end
end

function autoreload()
    for _, v in pairs(weapons:GetDescendants()) do
        if v.Name == "ReloadTime" then
            v.Value = 0.1
        end
    end
end

function fastfire()
    for _, v in pairs(weapons:GetDescendants()) do
        if v.Name == "FireRate" then
            v.Value = 0.05
        end
    end
end

modtab:Button("Infinite Ammo", function()
while wait() do
	game:GetService("Players").LocalPlayer.PlayerGui.GUI.Client.Variables.ammocount.Value = 999
	game:GetService("Players").LocalPlayer.PlayerGui.GUI.Client.Variables.ammocount2.Value = 999
end
end)

modtab:Button("Full Auto Weapons", function()
enableauto()
fastfire()
end)

modtab:Button("Instant Reload", function()
autoreload()
end)

modtab:Button("No Spread", function()
nospread()
end)

modtab:Button("No Recoil", function()
norecoil()
end)

misctab:Button("Infinite Yield", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
end)

