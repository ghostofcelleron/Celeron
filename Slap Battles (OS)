local function mix(item, count)
	for _ = 1, count do
		rs.AlchemistEvent:FireServer("MixItem", item)
	end
end

local function brewPotion(ingredients)
	for _, item in ipairs(ingredients) do
		rs.AlchemistEvent:FireServer("MixItem", item)
	end
	rs.AlchemistEvent:FireServer("BrewPotion")
end

local tabs = {
	Core = {
		{"Summon Cauldron", function() rs.GeneralAbility:FireServer() end},
		{"Get All Ingredients", function()
			local items = {
				"Blood Rose","Dark Root","Red Crystal","Blue Crystal","Glowing Mushroom",
				"Dire Flower","Wild Vine","Plane Flower","Jade Stone","Autumn Sprout",
				"Hazel Lily","Fire Flower","Winter Rose","Elder Wood","Mushroom","Cake Mix"
			}
			for _, i in ipairs(items) do
				for _ = 1, 50 do
					rs.AlchemistEvent:FireServer("AddItem", i)
				end
			end
		end},
		{"All Potions", function()
			mix("Blood Rose", 10)
			mix("Dark Root", 10)
			rs.AlchemistEvent:FireServer("BrewPotion")

brewPotion({"Red Crystal", "Dire Flower", "Wild Vine"})
