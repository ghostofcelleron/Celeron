exptab:Button("Instant Egg Quest (Accept Quest From Hitman First.)", function()
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")

local eggFolder = workspace:WaitForChild("EasterHuntEggs")
local basePosition = HumanoidRootPart.Position

local index = 0
for _, part in ipairs(eggFolder:GetDescendants()) do
	if part:IsA("MeshPart") and part.Anchored then
		part.Position = basePosition * index
