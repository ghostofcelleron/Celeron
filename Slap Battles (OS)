exptab:Label("Eggler Glove Exploits.")

local Players = game:GetService("Players")
local VirtualInputManager = game:GetService("VirtualInputManager")

local LocalPlayer = Players.LocalPlayer
local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
local HRP = Character:WaitForChild("HumanoidRootPart")

local escapePosition = Vector3.new(17944.336, -130.158, -3549.880)
local escapeRotation = Vector3.new(0.000, 179.367, 0.000)

local function stringSimilarity(a, b)
	a, b = a:lower(), b:lower()
	local score = 0
	for i = 1, math.min(#a, #b) do
		if a:sub(i, i) == b:sub(i, i) then
			score = score + 1
		end
	end
	return score
end

local function getBestMatch(name)
	local bestScore = -1
	local bestPlayer = nil

	for _, player in pairs(Players:GetPlayers()) do
		if player ~= LocalPlayer then
			local unameScore = stringSimilarity(name, player.Name)
			local dnameScore = stringSimilarity(name, player.DisplayName)
			local score = math.max(unameScore, dnameScore)

			if score > bestScore then
				bestScore = score
				bestPlayer = player
			end
		end
	end

	return bestPlayer
end

local function getLiveHRP(player)
	if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
		return player.Character.HumanoidRootPart
	end
end

local function teleportBehindAndFace(targetHRP)
	if not targetHRP then return end
	local backOffset = targetHRP.CFrame.LookVector * -3
	local newPos = targetHRP.Position + backOffset + Vector3.new(0, 2, 0)
	local lookAt = targetHRP.Position
	local newCFrame = CFrame.new(newPos, lookAt)
	HRP.CFrame = newCFrame
end

local function pressE()
	VirtualInputManager:SendKeyEvent(true, "E", false, game)
	task.wait(0.05)
	VirtualInputManager:SendKeyEvent(false, "E", false, game)
end

local function teleportToEscape()
	local rotationCFrame = CFrame.Angles(
		math.rad(escapeRotation.X),
		math.rad(escapeRotation.Y),
		math.rad(escapeRotation.Z)
	)
	HRP.CFrame = CFrame.new(escapePosition) * rotationCFrame
end

exptab:Textbox("Kick Player (Guide Portal)", function(v)
	local targetPlayer = getBestMatch(v)
	if targetPlayer then
		for i = 1, 3 do
			local targetHRP = getLiveHRP(targetPlayer)
			if targetHRP then
				teleportBehindAndFace(targetHRP)
				task.wait(0.05)
				pressE()
			end
			task.wait(0.05)
		end
		task.wait(0.2)
		teleportToEscape()
	end
end)

