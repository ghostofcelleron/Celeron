			Text = "Enabled.",
			Duration = 2
		})

		antiRingLoop = RunService.RenderStepped:Connect(function()
			local ring = workspace:FindFirstChild("Ring")
			if ring then
				ring:Destroy()
			end
		end)
	else
		StarterGui:SetCore("SendNotification", {
			Title = "Anti-Ring",
			Text = "Disabled.",
			Duration = 2
		})

		if antiRingLoop then
			antiRingLoop:Disconnect()
			antiRingLoop = nil
		end
	end
end)

