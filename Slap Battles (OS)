            while brickloop do
                pcall(function()
                    Remote:FireServer()
                end)
                task.wait(interval)
            end
            loopThread = nil
        end)
        coroutine.resume(loopThread)
    else
        notify("Brick Spam", "Disabled.")
    end
end)

