    firetouchinterest(hrp, part, 0)
    firetouchinterest(hrp, part, 1)
end

local function waitFor(partPath)
    local target
    repeat
        target = partPath()
        task.wait()
    until target
    return target
end

StarterGui:SetCore("SendNotification", {
    Title = "Hexa Glove",
    Text = "Obtaining Glove...",
    Duration = 10,
    Button1 = "alright fella",
})

