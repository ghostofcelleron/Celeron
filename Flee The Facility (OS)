tpstab:Label("In-Game Areas.")
local Players = game:GetService("Players")
local Workspace = game:GetService("Workspace")
local player = Players.LocalPlayer
local function getRoot() return player.Character or player.CharacterAdded:Wait(), "HumanoidRootPart" end

local function getRandomModel(name)
	local models = {}
	local function scan(parent)
		for _, obj in ipairs(parent:GetChildren()) do
			if obj:IsA("Model") and obj.Name == name then table.insert(models, obj) end
			if #obj:GetChildren() > 0 then scan(obj) end
		end
	end
	scan(Workspace)
	return #models > 0 and models[math.random(1, #models)] or nil
end

local recentComputers = {}

tpstab:Button("Random Computer TP", function()
	local model = getRandomModel("ComputerTable")
	if not model or not model.PrimaryPart then return end
	if table.find(recentComputers, model) then return end

	local char, rootName = getRoot()
	local root = char:WaitForChild(rootName)
	local front = model.PrimaryPart.CFrame.Position + model.PrimaryPart.CFrame.LookVector * 5
	root.CFrame = CFrame.new(front)

	table.insert(recentComputers, model)
	if #recentComputers > 3 then table.remove(recentComputers, 1) end
end)

tpstab:Button("Random Exit TP", function()
	local model = getRandomModel("ExitDoor")
	if not model or not model.PrimaryPart then return end

	local char, rootName = getRoot()
	local root = char:WaitForChild(rootName)
	local front = model.PrimaryPart.CFrame.Position + model.PrimaryPart.CFrame.LookVector * 5
	root.CFrame = CFrame.new(front)
end)

tpstab:Button("Random Freezer TP", function()
	local model = getRandomModel("FreezePod")
	if not model or not model.PrimaryPart then return end

	local char, rootName = getRoot()
	local root = char:WaitForChild(rootName)
	local front = model.PrimaryPart.CFrame.Position + model.PrimaryPart.CFrame.LookVector * 5
	root.CFrame = CFrame.new(front)
end)

