local ui = loadstring(game:HttpGet("https://pastebin.com/raw/wk7ZrGyr"))()

local win = ui:Create({
    Name = "Celeron's GUI (Pick A Side)",
    ThemeColor = Color3.fromRGB(20, 20, 20),
    StartupSound = "rbxassetid://6958727243",
    ThemeFont = Enum.Font.FredokaOne
})

local maintab = win:Tab("Main")
local farmtab = win:Tab("Farm")
local misctab = win:Tab("Others")
local helptab = win:Tab("Info")

maintab:Label("Script Made By Celeron + Daffy!")
farmtab:Label("Farming Section.")
helptab:Label("GUI Controls Are Below, Script Credits Are At The Bottom!")
helptab:Label("Show / Hide GUI: Right Alt")

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local function playerBoosts(character, propName, value)
    local humanoid = character:FindFirstChildOfClass("Humanoid")
    if not humanoid then return end
    local running = true
    task.spawn(function()
        while running and humanoid.Parent do
            humanoid[propName] = value
            task.wait(0.1)
        end
    end)
    return function() running = false end
end

do
    local stopLoop
    maintab:Textbox("Player Speed", function(v)
        if stopLoop then stopLoop() end
        local function apply(char)
            if stopLoop then stopLoop() end
            stopLoop = playerBoosts(char, "WalkSpeed", tonumber(v) or 16)
        end
        if LocalPlayer.Character then apply(LocalPlayer.Character) end
        LocalPlayer.CharacterAdded:Connect(apply)
    end)
end

do
    local stopLoop
    maintab:Textbox("Player Jump", function(v)
        if stopLoop then stopLoop() end
        local function apply(char)
            if stopLoop then stopLoop() end
            stopLoop = playerBoosts(char, "JumpPower", tonumber(v) or 50)
        end
        if LocalPlayer.Character then apply(LocalPlayer.Character) end
        LocalPlayer.CharacterAdded:Connect(apply)
    end)
end

maintab:Textbox("Weapon Reach", function(v)
    local char = LocalPlayer.Character
    if not char then return end
    local tool = char:FindFirstChildOfClass("Tool")
    if tool then
        local handle = tool:FindFirstChild("Handle")
        if handle and handle:IsA("BasePart") then
            local size = tonumber(v)
            if size then
                handle.Size = Vector3.new(size, size, size)
                handle.Massless = true
                handle.CanCollide = false
                pcall(function()
                    game:GetService("PhysicsService"):SetPartCollisionGroup(handle, "Default")
                end)
                local box = Instance.new("SelectionBox")
                box.Adornee = handle
                box.LineThickness = 0.05
                box.Color3 = Color3.fromRGB(11, 11, 11)
                box.Parent = handle
            end
        end
    end
end)

maintab:Label("Specific Weapon Features.")

maintab:Toggle("Camlock (Configured For Hex Spitter.)", function(v)
    local Players = game:GetService("Players")
    local RunService = game:GetService("RunService")
    local UserInputService = game:GetService("UserInputService")
    local StarterGui = game:GetService("StarterGui")
    local LocalPlayer = Players.LocalPlayer
    local Camera = workspace.CurrentCamera

    local rightClickHeld = false
    local lockedTarget = nil

    if getgenv()._camlockConnections then
        for _, conn in ipairs(getgenv()._camlockConnections) do
            conn:Disconnect()
        end
    end
    getgenv()._camlockConnections = {}

    local function getClosestEnemy()
        local closest, shortest = nil, math.huge
        for _, player in ipairs(Players:GetPlayers()) do
            if player ~= LocalPlayer and player.Team ~= LocalPlayer.Team then
                local char = player.Character
                local root = char and char:FindFirstChild("HumanoidRootPart")
                local hrp = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
                if root and hrp then
                    local dist = (hrp.Position - root.Position).Magnitude
                    if dist < shortest then
                        closest = root
                        shortest = dist
                    end
                end
            end
        end
        return closest
    end

    if v then
        StarterGui:SetCore("SendNotification", {Title = "Camlock", Text = "Enabled.", Duration = 3})
        table.insert(getgenv()._camlockConnections, UserInputService.InputBegan:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton2 then
                rightClickHeld = true
                lockedTarget = getClosestEnemy()
            end
        end))
        table.insert(getgenv()._camlockConnections, UserInputService.InputEnded:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton2 then
                rightClickHeld = false
                lockedTarget = nil
            end
        end))
        table.insert(getgenv()._camlockConnections, RunService.RenderStepped:Connect(function()
            if rightClickHeld and lockedTarget then
                Camera.CFrame = CFrame.new(Camera.CFrame.Position, lockedTarget.Position)
            end
        end))
    else
        StarterGui:SetCore("SendNotification", {Title = "Camlock", Text = "Disabled.", Duration = 3})
        for _, conn in ipairs(getgenv()._camlockConnections) do
            conn:Disconnect()
        end
        getgenv()._camlockConnections = {}
        rightClickHeld = false
        lockedTarget = nil
    end
end)

maintab:Toggle("Camlock (Configured For Golden Bow.)", function(v)
    local Players = game:GetService("Players")
    local RunService = game:GetService("RunService")
    local UserInputService = game:GetService("UserInputService")
    local StarterGui = game:GetService("StarterGui")
    local LocalPlayer = Players.LocalPlayer
    local Camera = workspace.CurrentCamera

    local predictionOffset = 2.225
    local rightClickHeld = false
    local lockedTarget = nil

    if getgenv()._camlockConnections then
        for _, conn in ipairs(getgenv()._camlockConnections) do
            conn:Disconnect()
        end
    end
    getgenv()._camlockConnections = {}

    local function getClosestEnemy()
        local closest, shortest = nil, math.huge
        for _, player in ipairs(Players:GetPlayers()) do
            if player ~= LocalPlayer and player.Team ~= LocalPlayer.Team then
                local char = player.Character
                local root = char and char:FindFirstChild("HumanoidRootPart")
                local hrp = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
                if root and hrp then
                    local dist = (hrp.Position - root.Position).Magnitude
                    if dist < shortest then
                        closest = root
                        shortest = dist
                    end
                end
            end
        end
        return closest
    end

    if v then
        StarterGui:SetCore("SendNotification", {Title = "Camlock", Text = "Enabled.", Duration = 3})
        table.insert(getgenv()._camlockConnections, UserInputService.InputBegan:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton2 then
                rightClickHeld = true
                lockedTarget = getClosestEnemy()
            end
        end))
        table.insert(getgenv()._camlockConnections, UserInputService.InputEnded:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton2 then
                rightClickHeld = false
                lockedTarget = nil
            end
        end))
        table.insert(getgenv()._camlockConnections, RunService.RenderStepped:Connect(function()
            if rightClickHeld and lockedTarget then
                local velocity = lockedTarget.Velocity
                local predictedPos = velocity.Magnitude > 0 and (lockedTarget.Position + velocity.Unit * predictionOffset) or lockedTarget.Position
                Camera.CFrame = CFrame.new(Camera.CFrame.Position, predictedPos)
            end
        end))
    else
        StarterGui:SetCore("SendNotification", {Title = "Camlock", Text = "Disabled.", Duration = 3})
        for _, conn in ipairs(getgenv()._camlockConnections) do
            conn:Disconnect()
        end
        getgenv()._camlockConnections = {}
        rightClickHeld = false
        lockedTarget = nil
    end
end)

local moneyFarmActive = false
local moneyFarmLoop = nil

farmtab:Toggle("Money Farm", function(v)
    moneyFarmActive = v
    local guiService = game:GetService("StarterGui")
    guiService:SetCore("SendNotification", {
        Title = "Money Farm",
        Text = moneyFarmActive and "Enabled." or "Disabled.",
        Duration = 3
    })

    if moneyFarmLoop then
        moneyFarmLoop:Disconnect()
        moneyFarmLoop = nil
    end

    if moneyFarmActive then
        moneyFarmLoop = game:GetService("RunService").Heartbeat:Connect(function()
            local character = LocalPlayer.Character
            local rootPart = character and character:FindFirstChild("HumanoidRootPart")
            if rootPart then
                rootPart.CFrame = CFrame.new(50, 4, 7)
            end
        end)
    end
end)

misctab:Button("Infinite Yield", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
end)

