local Players = game:GetService("Players")
local StarterGui = game:GetService("StarterGui")
local CoreGui = game:GetService("CoreGui")

local playerESPEnabled = false

local function notify(title, text)
    StarterGui:SetCore("SendNotification", {
        Title = title,
        Text = text,
        Duration = 3
    })
end

local function applyHighlight(player)
    if player == Players.LocalPlayer then return end
    local char = player.Character
    if not char or not char:IsA("Model") then return end

    local existingHighlight = CoreGui:FindFirstChild("ESP_" .. player.Name)

    if playerESPEnabled then
        if not existingHighlight then
            local highlight = Instance.new("Highlight")
            highlight.Name = "ESP_" .. player.Name
            highlight.Adornee = char
            highlight.FillColor = Color3.fromRGB(255, 1, 1)
            highlight.OutlineColor = Color3.new(1, 1, 1)
            highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
            highlight.Parent = CoreGui
        end
        local humanoid = char:FindFirstChild("Humanoid")
        if humanoid then
            humanoid.DisplayDistanceType = Enum.HumanoidDisplayDistanceType.None
            humanoid.NameDisplayDistance = math.huge
        end
    else
        if existingHighlight then existingHighlight:Destroy() end
        local humanoid = char:FindFirstChild("Humanoid")
        if humanoid then
            humanoid.DisplayDistanceType = Enum.HumanoidDisplayDistanceType.Viewer
            humanoid.NameDisplayDistance = 100
        end
    end
end

vistab:Toggle("Player ESP", function()
    playerESPEnabled = not playerESPEnabled
    notify("Player ESP", playerESPEnabled and "Enabled." or "Disabled.")

    for _, player in ipairs(Players:GetPlayers()) do
        applyHighlight(player)
    end

    Players.PlayerAdded:Connect(function(player)
        player.CharacterAdded:Connect(function()
            if playerESPEnabled then
                task.wait(1)
                applyHighlight(player)
            end
        end)
    end)
end)

vistab:Toggle("Always Night", function()
	getgenv().AlwaysNight = not getgenv().AlwaysNight

	local Lighting = game:GetService("Lighting")

	if getgenv().AlwaysNight then
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Always Night",
			Text = "Enabled.",
			Duration = 3
		})

		Lighting.ClockTime = 24
		Lighting.TimeOfDay = "00:00:00"
		Lighting:SetMinutesAfterMidnight(0)

		if not getgenv().AlwaysNightConnection then
			getgenv().AlwaysNightConnection = Lighting:GetPropertyChangedSignal("ClockTime"):Connect(function()
				if getgenv().AlwaysNight then
					Lighting.ClockTime = 24
				end
			end)
		end
	else
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Always Night",
			Text = "Disabled.",
			Duration = 3
		})

		if getgenv().AlwaysNightConnection then
			getgenv().AlwaysNightConnection:Disconnect()
			getgenv().AlwaysNightConnection = nil
		end
	end
end)

misctab:Button("Infinite Yield", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
end)

