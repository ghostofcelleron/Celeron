local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local StarterGui = game:GetService("StarterGui")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

local normal = 2809202155
local sbr = 4643697430

if game.PlaceId == normal then

local ui = loadstring(game:HttpGet("https://raw.githubusercontent.com/Daffy734/code/refs/heads/main/yep"))()

local win = ui:Create({
    Name = "Celeron's GUI (YBA)",
    ThemeColor = Color3.fromRGB(80, 0, 145),
    StartupSound = "rbxassetid://6958727243",
    ThemeFont = Enum.Font.GothamBold
})

local maintab = win:Tab("Main")
local vistab = win:Tab("Visuals")
local maptab = win:Tab("Teleports")
local misctab = win:Tab("Others")
local helptab = win:Tab("Info")

maintab:Label("Script Made By Celeron + Daffy!")
vistab:Label("Visual Settings.")
maptab:Label("Teleport Features, May Be Buggy.")
misctab:Label("Other Features.")
helptab:Label("GUI Controls Are Below, Script Credits Are At The Bottom!")
helptab:Label("Show / Hide GUI: Right Alt")

local player = game:GetService("Players").LocalPlayer
local stepSize = 40
local delayTime = 0.075
local activeTeleport = false

local function getHRP()
	local char = player.Character or player.CharacterAdded:Wait()
	return char:WaitForChild("HumanoidRootPart")
end

local function freeze(hrp)
	local bv = Instance.new("BodyVelocity")
	bv.Name = "teleportvelocity"
	bv.MaxForce = Vector3.new(1e5, 1e5, 1e5)
	bv.Velocity = Vector3.zero
	bv.P = 1e4
	bv.Parent = hrp

	local bg = Instance.new("BodyGyro")
	bg.Name = "teleportgyro"
	bg.MaxTorque = Vector3.new(1e5, 1e5, 1e5)
	bg.CFrame = hrp.CFrame
	bg.P = 1e4
	bg.Parent = hrp
end

local function unfreeze(hrp)
	for _, obj in ipairs(hrp:GetChildren()) do
		if obj:IsA("BodyVelocity") and obj.Name == "teleportvelocity" then
			obj:Destroy()
		elseif obj:IsA("BodyGyro") and obj.Name == "teleportgyro" then
			obj:Destroy()
		end
	end
end

local function forceNoclip()
	local char = player.Character
	if not char then return end
	for _, part in ipairs(char:GetDescendants()) do
		if part:IsA("BasePart") then
			part.CanCollide = false
		end
	end
end

local function restoreCollisions()
	local char = player.Character
	if not char then return end
	for _, part in ipairs(char:GetDescendants()) do
		if part:IsA("BasePart") then
			part.CanCollide = true
		end
	end
end

player.CharacterAdded:Connect(function(char)
	if activeTeleport then
		task.wait()
		local hrp = char:FindFirstChild("HumanoidRootPart")
		if hrp then
			unfreeze(hrp)
		end
		restoreCollisions()
		activeTeleport = false
	end
end)

function teleportto(x, y, z)
	activeTeleport = true

	task.spawn(function()
		local hrp = getHRP()
		freeze(hrp)

		local running = true
		local noclipLoop = coroutine.wrap(function()
			while running and activeTeleport do
				forceNoclip()
				task.wait()
			end
		end)
		noclipLoop()

		while math.abs(getHRP().Position.Y - y) > 2 and activeTeleport do
			local hrp = getHRP()
			local current = hrp.Position
			local nextY = current.Y + math.sign(y - current.Y) * math.min(stepSize, math.abs(y - current.Y))
			hrp.CFrame = CFrame.new(current.X, nextY, current.Z)
			task.wait(delayTime)
		end

		while (Vector3.new(getHRP().Position.X, 0, getHRP().Position.Z) - Vector3.new(x, 0, z)).Magnitude > 2 and activeTeleport do
			local hrp = getHRP()
			local current = hrp.Position
			local targetXZ = Vector3.new(x, current.Y, z)
			local direction = (targetXZ - current).Unit
			local step = direction * math.min(stepSize, (targetXZ - current).Magnitude)
			hrp.CFrame = CFrame.new(current + step)
			task.wait(delayTime)
		end

		running = false
		if activeTeleport then
			unfreeze(getHRP())
			restoreCollisions()
			activeTeleport = false
		end
	end)
end

getgenv().lastSliderSet = nil
getgenv().cframeLoopRunning = false

local uis = game:GetService("UserInputService")
local players = game:GetService("Players")
local workspace = game:GetService("Workspace")
local lp = players.LocalPlayer

local function getInputDirection()
	local dir = Vector3.zero
	if uis:IsKeyDown(Enum.KeyCode.W) then dir += Vector3.new(0, 0, 1) end
	if uis:IsKeyDown(Enum.KeyCode.S) then dir += Vector3.new(0, 0, -1) end
	if uis:IsKeyDown(Enum.KeyCode.A) then dir += Vector3.new(-1, 0, 0) end
	if uis:IsKeyDown(Enum.KeyCode.D) then dir += Vector3.new(1, 0, 0) end
	return dir.Magnitude > 0 and dir.Unit or nil
end

local function createBodyVelocity(hrp)
	local bv = Instance.new("BodyVelocity")
	bv.Name = "Velocity"
	bv.MaxForce = Vector3.new(1e5, 0, 1e5)
	bv.Velocity = Vector3.zero
	bv.P = 1e4
	bv.Parent = hrp
	return bv
end

getgenv().applySpeed = function()
	if getgenv().cframeLoopRunning then return end
	getgenv().cframeLoopRunning = true

	coroutine.wrap(function()
		while getgenv().cframeLoopRunning do
			local char = lp.Character
			local hrp = char and char:FindFirstChild("HumanoidRootPart")
			local hum = char and char:FindFirstChildOfClass("Humanoid")
			local cam = workspace.CurrentCamera
			if hrp and hum then
				local speed = getgenv().lastSliderSet or 16
				local bv = hrp:FindFirstChild("Velocity")

				if speed < 19 then
					if bv then bv:Destroy() end
				else
					bv = bv or createBodyVelocity(hrp)
					local inputDir = getInputDirection()
					if inputDir then
						local camCF = cam.CFrame
						local forward = Vector3.new(camCF.LookVector.X, 0, camCF.LookVector.Z).Unit
						local right = Vector3.new(camCF.RightVector.X, 0, camCF.RightVector.Z).Unit
						local moveVec = (forward * inputDir.Z + right * inputDir.X).Unit
						local currentY = hrp.Velocity.Y

						bv.Velocity = moveVec * speed + Vector3.new(0, currentY, 0)
					else
						bv.Velocity = Vector3.zero
					end
				end
			end
			task.wait()
		end
	end)()
end

maintab:Slider("Player Speed", 20, 0, 240, 1, function(v)
	getgenv().lastSliderSet = (v == 8) and 16 or v
	getgenv().applySpeed()
end)

players.LocalPlayer.CharacterAdded:Connect(function(char)
	char:WaitForChild("Humanoid")
	getgenv().applySpeed()
end)

getgenv().lastJumpSet = nil
getgenv().jumpBoostEnabled = true

local players = game:GetService("Players")
local lp = players.LocalPlayer

local function applyJumpBoost(hum, hrp)
	hum.StateChanged:Connect(function(_, new)
		if new == Enum.HumanoidStateType.Jumping and getgenv().jumpBoostEnabled then
			local boost = Instance.new("BodyVelocity")
			boost.Name = "jump"
			boost.MaxForce = Vector3.new(0, 1e5, 0)
			boost.Velocity = Vector3.new(0, getgenv().lastJumpSet or 50, 0)
			boost.P = 1e4
			boost.Parent = hrp

			game:GetService("Debris"):AddItem(boost, 0.2)
		end
	end)
end

getgenv().applyJump = function()
	local char = lp.Character
	local hum = char and char:FindFirstChildOfClass("Humanoid")
	local hrp = char and char:FindFirstChild("HumanoidRootPart")
	if hum and hrp then
		hum.JumpPower = 50
		applyJumpBoost(hum, hrp)
	end
end

maintab:Slider("Player Jump", 50, 0, 350, 1, function(v)
	getgenv().lastJumpSet = v
	getgenv().applyJump()
end)

players.LocalPlayer.CharacterAdded:Connect(function(char)
	char:WaitForChild("Humanoid")
	char:WaitForChild("HumanoidRootPart")
	getgenv().applyJump()
end)

maintab:Button("Teleport Tool", function()
    local Players = game:GetService("Players")

    local tool = Instance.new("Tool")
    tool.Name = "Teleport Tool"
    tool.ToolTip = "lets u teleport anywhere u click"
    tool.RequiresHandle = false
	
    local player = Players.LocalPlayer
    tool.Parent = player:WaitForChild("Backpack")

    tool.Equipped:Connect(function(mouse)
        mouse.Button1Down:Connect(function()
            local pos = mouse.Hit.Position
            teleportto(pos.X, pos.Y, pos.Z)
        end)
    end)
end)

local itemESPEnabled = false
local activeESP = {}
local lastItemNames = {}
local currentRoot = nil

vistab:Toggle("Item ESP", function()
	itemESPEnabled = not itemESPEnabled

	local StarterGui = game:GetService("StarterGui")
	StarterGui:SetCore("SendNotification", {
		Title = "Item ESP",
		Text = itemESPEnabled and "Enabled." or "Disabled.",
		Duration = 3
	})

	local player = game.Players.LocalPlayer
	local itemsFolder = workspace:FindFirstChild("Item_Spawns") and workspace.Item_Spawns:FindFirstChild("Items")
	if not itemsFolder then return end

	local function updateRoot()
		local char = player.Character or player.CharacterAdded:Wait()
		local root = char:WaitForChild("HumanoidRootPart")
		currentRoot = root

		local humanoid = char:FindFirstChildOfClass("Humanoid")
		if humanoid then
			humanoid.Died:Connect(function()
				currentRoot = nil
				player.CharacterAdded:Wait():WaitForChild("HumanoidRootPart")
				task.wait(0.5)
				updateRoot()
				if itemESPEnabled then
					createESP()
				end
			end)
		end

		root:GetPropertyChangedSignal("Parent"):Connect(function()
			if not root:IsDescendantOf(workspace) then
				task.wait(0.5)
				updateRoot()
			end
		end)
	end

	updateRoot()
	player.CharacterAdded:Connect(function()
		task.wait(0.5)
		updateRoot()
		if itemESPEnabled then
			createESP()
		end
	end)

	local function clearESP()
		for _, gui in pairs(activeESP) do
			if gui and gui.Parent then
				gui:Destroy()
			end
		end
		activeESP = {}
	end

	local function getPromptText(model)
		for _, inst in ipairs(model:GetDescendants()) do
			if inst:IsA("ProximityPrompt") and inst.ObjectText and inst.ObjectText ~= "" then
				return inst.ObjectText
			end
		end
		return model.Name
	end

	local function createESP()
		clearESP()
		lastItemNames = {}
		for _, obj in ipairs(itemsFolder:GetChildren()) do
			if obj:IsA("Model") or obj:IsA("BasePart") then
				local itemName = getPromptText(obj)
				table.insert(lastItemNames, itemName)

				local part = obj:IsA("BasePart") and obj or obj:FindFirstChildWhichIsA("BasePart")
				if part then
					local gui = Instance.new("BillboardGui")
					gui.Size = UDim2.new(0, 100, 0, 12)
					gui.StudsOffset = Vector3.new(0, 2.5, 0)
					gui.AlwaysOnTop = true
					gui.Adornee = part
					gui.Parent = part

					local nameLabel = Instance.new("TextLabel")
					nameLabel.Size = UDim2.new(1, 0, 1, 0)
					nameLabel.BackgroundTransparency = 1
					nameLabel.TextColor3 = Color3.new(1, 1, 1)
					nameLabel.TextScaled = true
					nameLabel.Font = Enum.Font.Arial
					nameLabel.Text = itemName
					nameLabel.TextSize = 8
					nameLabel.Parent = gui

					table.insert(activeESP, gui)
				end
			end
		end
	end

	local function itemsChanged()
		local currentNames = {}
		for _, obj in ipairs(itemsFolder:GetChildren()) do
			if obj:IsA("Model") or obj:IsA("BasePart") then
				table.insert(currentNames, getPromptText(obj))
			end
		end
		if #currentNames ~= #lastItemNames then return true end
		for i, name in ipairs(currentNames) do
			if name ~= lastItemNames[i] then return true end
		end
		return false
	end

	task.spawn(function()
		while true do
			if itemESPEnabled and itemsChanged() then
				createESP()
			end
			task.wait(3)
		end
	end)

	task.spawn(function()
		while true do
			if itemESPEnabled then
				createESP()
			end
			task.wait(5)
		end
	end)

	if itemESPEnabled then
		createESP()
	else
		clearESP()
	end
end)

vistab:Toggle("Always Night", function()
	getgenv().AlwaysNight = not getgenv().AlwaysNight

	local Lighting = game:GetService("Lighting")

	if getgenv().AlwaysNight then
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Always Night",
			Text = "Enabled.",
			Duration = 3
		})

		Lighting.ClockTime = 24
		Lighting.TimeOfDay = "00:00:00"
		Lighting:SetMinutesAfterMidnight(0)

		if not getgenv().AlwaysNightConnection then
			getgenv().AlwaysNightConnection = Lighting:GetPropertyChangedSignal("ClockTime"):Connect(function()
				if getgenv().AlwaysNight then
					Lighting.ClockTime = 24
				end
			end)
		end
	else
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Always Night",
			Text = "Disabled.",
			Duration = 3
		})

		if getgenv().AlwaysNightConnection then
			getgenv().AlwaysNightConnection:Disconnect()
			getgenv().AlwaysNightConnection = nil
		end
	end
end)

