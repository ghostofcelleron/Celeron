local LocalPlayer = Players.LocalPlayer

blatant:CreateSection("Visuals & Misc")
blatant:CreateButton({ Name = "Metavision", Description = "Load Metavision", Callback = function() loadstring(game:HttpGet("https://pastebin.com/raw/FVgs7bQw"))() end })
