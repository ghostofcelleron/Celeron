    if gp or input.KeyCode ~= Enum.KeyCode.LeftAlt then return end
    if not airDribbleEnabled then return end
    local char = Players.LocalPlayer.Character
    local hum = char and char:FindFirstChildOfClass("Humanoid")
    local root = char and char:FindFirstChild("HumanoidRootPart")
    if not (hum and root) then return end
    local anim = Instance.new("Animation"); anim.AnimationId = "rbxassetid://76587445975710"
    local track = hum:LoadAnimation(anim); track:Play()
    local dir = root.CFrame.LookVector
    local kickVec = vector.create(dir.X * 0.75, 0.65, dir.Z * 0.75)
    ReplicatedStorage:WaitForChild("ByteNetReliable"):FireServer(buffer.fromstring("\034\001"), { { "kick", 28, false, kickVec } })
