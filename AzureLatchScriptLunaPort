_G.AUTO_GOAL = _G.AUTO_GOAL or false
blatant:CreateToggle({
    Name = "Auto Goal",
    Description = "Automatically steal and score if possible.",
    CurrentValue = _G.AUTO_GOAL,
    Callback = function(v)
        _G.AUTO_GOAL = v
        notify("Auto Goal", _G.AUTO_GOAL and "Enabled." or "Disabled.", 1)
    end
})

do
    local MapFolderName = "map"
    local function IsInGame()
        local LocalCharacter = LocalPlayer.Character
        if not LocalCharacter then return false end
        local StateFolder = LocalCharacter:FindFirstChild("state")
        if not StateFolder then return false end
        local InGameValue = StateFolder:FindFirstChild("ingame")
        if not InGameValue then return false end
        return InGameValue.Value
    end

    local function DisableCollisionBoxes()
        local MapFolder = Workspace:FindFirstChild(MapFolderName)
        if not MapFolder then return end
        local GkBarriar = MapFolder:FindFirstChild("gkbarriar")
        local AGoal = MapFolder:FindFirstChild("Agoal")
        local BGoal = MapFolder:FindFirstChild("Bgoal")
        if GkBarriar then
            local ABarriar = GkBarriar:FindFirstChild("A")
            local BBarriar = GkBarriar:FindFirstChild("B")
            if ABarriar then ABarriar.CanCollide = false end
            if BBarriar then BBarriar.CanCollide = false end
        end
        if AGoal then AGoal.CanCollide = false end
        if BGoal then BGoal.CanCollide = false end
    end

    local function StealBall()
        local LocalCharacter = LocalPlayer.Character
        local LocalHumanoidRootPart = LocalCharacter and LocalCharacter:FindFirstChild("HumanoidRootPart")
        local Football = Workspace.Terrain:FindFirstChild("Ball")
        if LocalHumanoidRootPart and Football then
            LocalHumanoidRootPart.CFrame = CFrame.new(Football.Position.X, 0, Football.Position.Z)
        end
        for _, OtherPlayer in pairs(Players:GetPlayers()) do
            if OtherPlayer ~= LocalPlayer then
                local OtherCharacter = OtherPlayer.Character
                local OtherFootball = OtherCharacter and OtherCharacter:FindFirstChild("Ball")
                local OtherHumanoidRootPart = OtherCharacter and OtherCharacter:FindFirstChild("HumanoidRootPart")
                if OtherFootball and OtherHumanoidRootPart and LocalHumanoidRootPart then
                    LocalHumanoidRootPart.CFrame = OtherFootball.CFrame
                    local args = { buffer.fromstring("\034\001"), { { "tackle" } } }
                    ReplicatedStorage:WaitForChild("ByteNetReliable"):FireServer(unpack(args))
                end
            end
        end
    end

    local function HasBall()
        local LocalCharacter = LocalPlayer.Character
        local Football = LocalCharacter and LocalCharacter:FindFirstChild("Ball")
        return Football ~= nil
    end

    coroutine.resume(coroutine.create(function()
        RunService.RenderStepped:Connect(function()
            if not _G.AUTO_GOAL then return end
            pcall(function()
                if not IsInGame() then return end
                DisableCollisionBoxes()
                StealBall()
                if HasBall() then
                    local LocalCharacter = LocalPlayer.Character
                    local LocalRootPart = LocalCharacter and LocalCharacter:FindFirstChild("HumanoidRootPart")
                    local MapFolder = Workspace:FindFirstChild(MapFolderName)
                    local AGoal = MapFolder and MapFolder:FindFirstChild("Agoal")
                    local BGoal = MapFolder and MapFolder:FindFirstChild("Bgoal")
                    local Goal = LocalPlayer.Team and ((LocalPlayer.Team.Name == "A") and BGoal or AGoal)
                    if LocalRootPart and Goal then
                        LocalRootPart.CFrame = Goal.CFrame
                        wait(0.185)
                        local args = { buffer.fromstring("\034\001"), { { "kick", 20, false, vector.create(0,1,0) } } }
                        ReplicatedStorage:WaitForChild("ByteNetReliable"):FireServer(unpack(args))
                    end
                end
            end)
        end)
    end))
end

_G.ALWAYS_BALL_ACTIVE = _G.ALWAYS_BALL_ACTIVE or false
blatant:CreateToggle({
    Name = "Always Ball",
    Description = "Stay attached to the ball (zero gravity while active).",
    CurrentValue = _G.ALWAYS_BALL_ACTIVE,
    Callback = function(v)
        _G.ALWAYS_BALL_ACTIVE = v
        if v then
            notify("Always Ball", "Enabled.", 1)
            workspace.Gravity = 0
            spawn(function()
                while _G.ALWAYS_BALL_ACTIVE do
                    local LocalCharacter = LocalPlayer.Character
                    local LocalHumanoidRootPart = LocalCharacter and LocalCharacter:FindFirstChild("HumanoidRootPart")
                    local Football = Workspace.Terrain:FindFirstChild("Ball")
                    if LocalHumanoidRootPart and Football then
                        LocalHumanoidRootPart.CFrame = CFrame.new(Football.Position)
                    end
                    for _, OtherPlayer in pairs(Players:GetPlayers()) do
                        if OtherPlayer ~= LocalPlayer then
                            local OtherCharacter = OtherPlayer.Character
                            local OtherFootball = OtherCharacter and OtherCharacter:FindFirstChild("Ball")
                            local OtherHumanoidRootPart = OtherCharacter and OtherCharacter:FindFirstChild("HumanoidRootPart")
                            if OtherFootball and OtherHumanoidRootPart and LocalHumanoidRootPart then
                                LocalHumanoidRootPart.CFrame = OtherFootball.CFrame
                                local args = { buffer.fromstring("\034\001"), { { "tackle" } } }
                                ReplicatedStorage:WaitForChild("ByteNetReliable"):FireServer(unpack(args))
                            end
                        end
                    end
                    task.wait(0.1)
                end
            end)
        else
            notify("Always Ball", "Disabled.", 1)
            workspace.Gravity = 196.2
        end
    end
})

blatant:CreateButton({
    Name = "Get Flow (Req. Ball)",
    Description = "Spam-kicks to reach flow 100 (requires ball).",
    Callback = function()
        local player = Players.LocalPlayer
        local char = player.Character or player.CharacterAdded:Wait()
        local root = char:WaitForChild("HumanoidRootPart")
        local original = root.CFrame
        local sword = Workspace:FindFirstChild("map") and Workspace.map.Lobby_Museum.Prop and Workspace.map.Lobby_Museum.Prop["Classic Roblox Sword"]
        if sword then root.CFrame = sword.CFrame + Vector3.new(0,33,0) end
        wait(0.67)
        task.spawn(function()
            local remote = ReplicatedStorage:WaitForChild("ByteNetReliable")
            local flow = Workspace:WaitForChild("characters"):WaitForChild(player.Name):WaitForChild("state"):WaitForChild("flow")
            while flow.Value < 100 do
                remote:FireServer(buffer.fromstring("\034\001"), { { "kick", 1, false, vector.create(0,1,0) } })
                task.wait(0.01)
            end
            wait(0.9)
            root.CFrame = original
        end)
    end
})

local isEnabled = false
_G.ForwardRushEnabled = _G.ForwardRushEnabled or false
_G.SideDashEnabled = _G.SideDashEnabled or false
_G._rushing = _G._rushing or false
_G._sideDashing = _G._sideDashing or false
_G._dashBindsSetup = _G._dashBindsSetup or false
_G._lastForwardRush = _G._lastForwardRush or 0
_G._lastSideDash = _G._lastSideDash or 0

local player = Players.LocalPlayer
local character, humanoid, rootPart
local function updateCharacter()
    character = player.Character or player.CharacterAdded:Wait()
    humanoid = character:WaitForChild("Humanoid")
    rootPart = character:WaitForChild("HumanoidRootPart")
end
updateCharacter()
player.CharacterAdded:Connect(updateCharacter)

local function forwardRush()
    if _G._rushing or not _G.ForwardRushEnabled or not humanoid or not rootPart then return end
    if tick() - _G._lastForwardRush < 0.2 then return end
    _G._lastForwardRush = tick()
    _G._rushing = true
    local animator = humanoid:FindFirstChildOfClass("Animator") or Instance.new("Animator", humanoid)
    local animation = Instance.new("Animation"); animation.AnimationId = "rbxassetid://79394729551302"
    local animationTrack = animator:LoadAnimation(animation); animationTrack:Play()
    task.delay(0.05, function()
        if animationTrack.IsPlaying then
            local sound = Instance.new("Sound"); sound.SoundId = "rbxassetid://105267293181745"; sound.Parent = rootPart; sound:Play()
            sound.Ended:Connect(function() sound:Destroy() end)
        end
    end)
    local bv = Instance.new("BodyVelocity"); bv.MaxForce = Vector3.new(1e5,0,1e5); bv.Parent = rootPart
    local dashId = "ForwardRush_" .. tostring(os.clock())
    RunService:BindToRenderStep(dashId, Enum.RenderPriority.Character.Value, function()
        if not rootPart or not rootPart.Parent then RunService:UnbindFromRenderStep(dashId); bv:Destroy(); _G._rushing = false; return end
        bv.Velocity = rootPart.CFrame.LookVector * 100
    end)
    task.delay(0.5, function() RunService:UnbindFromRenderStep(dashId); bv:Destroy(); _G._rushing = false end)
end

local function sideDash(direction)
    if _G._sideDashing or not _G.SideDashEnabled or not humanoid or not rootPart then return end
    if tick() - _G._lastSideDash < 0.15 then return end
    _G._lastSideDash = tick()
    _G._sideDashing = true
    local animator = humanoid:FindFirstChildOfClass("Animator") or Instance.new("Animator", humanoid)
    local animation = Instance.new("Animation")
    animation.AnimationId = direction == "right" and "rbxassetid://114016332539655" or "rbxassetid://100207093237932"
    local animationTrack = animator:LoadAnimation(animation); animationTrack:Play()
    local sound = Instance.new("Sound"); sound.SoundId = "rbxassetid://71212694698006"; sound.Parent = rootPart; sound:Play()
    sound.Ended:Connect(function() sound:Destroy() end)
    local bv = Instance.new("BodyVelocity"); bv.MaxForce = Vector3.new(1e5,0,1e5); bv.Parent = rootPart
    local dashId = "SideDash_" .. tostring(os.clock())
    RunService:BindToRenderStep(dashId, Enum.RenderPriority.Character.Value, function()
        if not rootPart or not rootPart.Parent then RunService:UnbindFromRenderStep(dashId); bv:Destroy(); _G._sideDashing = false; return end
        local rightVec = rootPart.CFrame.RightVector
        bv.Velocity = rightVec * (direction == "right" and 75 or -75)
    end)
    task.delay(0.4, function() RunService:UnbindFromRenderStep(dashId); bv:Destroy(); _G._sideDashing = false end)
end

blatant:CreateLabel({ Text = "No Cooldown Features.", Style = 1 })
blatant:CreateToggle({ Name = "No Rush Cooldown", Description = "Enable forward rush", CurrentValue = _G.ForwardRushEnabled, Callback = function(v) _G.ForwardRushEnabled = v notify("No Rush CD", v and "Enabled." or "Disabled.") end })
blatant:CreateToggle({ Name = "No Side Dash Cooldown", Description = "Enable side dash", CurrentValue = _G.SideDashEnabled, Callback = function(v) _G.SideDashEnabled = v notify("No Side Dash CD", v and "Enabled." or "Disabled.") end })

if not _G._dashBindsSetup then
    _G._dashBindsSetup = true
    UserInputService.InputBegan:Connect(function(input, gameProcessed)
        if gameProcessed then return end
        if input.KeyCode == Enum.KeyCode.F then forwardRush()
        elseif input.KeyCode == Enum.KeyCode.Q then
            local isA = UserInputService:IsKeyDown(Enum.KeyCode.A)
            local isD = UserInputService:IsKeyDown(Enum.KeyCode.D)
            if isD then sideDash("right") elseif isA then sideDash("left") end
        end
    end)
end

blatant:CreateSection("Break Ball")
blatant:CreateButton({ Name = "Break Ball (Player Method, Req. Ball)", Description = "Creates far-away parts and teleports you high.", Callback = function()
    local baseplatePosition = Vector3.new(-190, 14864566, 492)
    local partSize = Vector3.new(10,1,10)
    local gap = 0
    for x = 0, 2 do for z = 0, 2 do
        local part = Instance.new("Part"); part.Size = partSize; part.Anchored = true
        part.Position = baseplatePosition + Vector3.new(x * (partSize.X + gap), 0, z * (partSize.Z + gap))
        part.Parent = workspace
    end end
    local player = Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    character:WaitForChild("HumanoidRootPart").CFrame = CFrame.new(baseplatePosition + Vector3.new(0, 100000000000000, 0))
end })
blatant:CreateButton({ Name = "Break Ball (Ball Method, Req. Ball)", Description = "Temporarily move you to a spot and jump to break the ball.", Callback = function()
    local player = Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
    if humanoidRootPart then
        local originalPosition = humanoidRootPart.Position
        humanoidRootPart.CFrame = CFrame.new(Vector3.new(-390, 475, 354))
        task.wait(0.3)
        VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.Space, false, game)
        task.wait(0.1)
        VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.Space, false, game)
        task.wait(0.3)
        humanoidRootPart.CFrame = CFrame.new(originalPosition)
    end
end })
blatant:CreateButton({ Name = "Permanent Break Ball (Req. Ball)", Description = "Permanently lower fallen parts destroy height and move player", Callback = function()
    workspace.FallenPartsDestroyHeight = -50000
    pcall(function() game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(CFrame.new(1, -49999, 1)) end)
end })

blatant:CreateSection("Ball Control")
blatant:CreateButton({ Name = "Steal Ball", Description = "Attempts to steal the ball until you have it", Callback = function()
    local LocalPlayer = Players.LocalPlayer
    local function HasBall() local LocalCharacter = LocalPlayer.Character; local Football = LocalCharacter and LocalCharacter:FindFirstChild("Ball"); return Football ~= nil end
    local function StealBall()
        while not HasBall() do
            local LocalCharacter = LocalPlayer.Character
            local LocalHumanoidRootPart = LocalCharacter and LocalCharacter:FindFirstChild("HumanoidRootPart")
            local Football = Workspace.Terrain:FindFirstChild("Ball")
            if LocalHumanoidRootPart and Football then LocalHumanoidRootPart.CFrame = CFrame.new(Football.Position) end
            for _, OtherPlayer in pairs(Players:GetPlayers()) do
                if OtherPlayer ~= LocalPlayer then
                    local OtherCharacter = OtherPlayer.Character
                    local OtherFootball = OtherCharacter and OtherCharacter:FindFirstChild("Ball")
                    local OtherHumanoidRootPart = OtherCharacter and OtherCharacter:FindFirstChild("HumanoidRootPart")
                    if OtherFootball and OtherHumanoidRootPart and LocalHumanoidRootPart then
                        LocalHumanoidRootPart.CFrame = OtherFootball.CFrame
                        local args = { buffer.fromstring("\034\001"), { { "tackle" } } }
                        ReplicatedStorage:WaitForChild("ByteNetReliable"):FireServer(unpack(args))
                    end
                end
            end
            task.wait(0.2)
        end
    end
    StealBall()
end })
blatant:CreateButton({ Name = "Bring Ball", Description = "Bring the ball to you briefly", Callback = function()
    local LocalPlayer = Players.LocalPlayer
    local function HasBall() local LocalCharacter = LocalPlayer.Character; local Football = LocalCharacter and LocalCharacter:FindFirstChild("Ball"); return Football ~= nil end
    local function StealBall()
        while not HasBall() do
            local LocalCharacter = LocalPlayer.Character
            local LocalHumanoidRootPart = LocalCharacter and LocalCharacter:FindFirstChild("HumanoidRootPart")
            local Football = Workspace.Terrain:FindFirstChild("Ball")
            if LocalHumanoidRootPart and Football then LocalHumanoidRootPart.CFrame = CFrame.new(Football.Position) end
            for _, OtherPlayer in pairs(Players:GetPlayers()) do
                if OtherPlayer ~= LocalPlayer then
                    local OtherCharacter = OtherPlayer.Character
                    local OtherFootball = OtherCharacter and OtherCharacter:FindFirstChild("Ball")
                    local OtherHumanoidRootPart = OtherCharacter and OtherCharacter:FindFirstChild("HumanoidRootPart")
                    if OtherFootball and OtherHumanoidRootPart and LocalHumanoidRootPart then
                        LocalHumanoidRootPart.CFrame = OtherFootball.CFrame
                        local args = { buffer.fromstring("\034\001"), { { "tackle" } } }
                        ReplicatedStorage:WaitForChild("ByteNetReliable"):FireServer(unpack(args))
                    end
                end
            end
            task.wait(0.2)
        end
    end
    spawn(function()
        local LocalCharacter = LocalPlayer.Character
        local LocalHumanoidRootPart = LocalCharacter and LocalCharacter:FindFirstChild("HumanoidRootPart")
        local Football = Workspace.Terrain:FindFirstChild("Ball")
        if LocalHumanoidRootPart and Football then
            local originalPosition = LocalHumanoidRootPart.CFrame
            LocalHumanoidRootPart.CFrame = CFrame.new(Football.Position)
            StealBall()
            task.wait(0.3)
            LocalHumanoidRootPart.CFrame = originalPosition
        end
    end)
end })

silent:CreateSection("Ball Magnet")
getgenv().ballMagnetActive = getgenv().ballMagnetActive or false
silent:CreateToggle({
    Name = "Ball Magnet (Auto Fire)",
    Description = "Continuously fire the magnet remote.",
    CurrentValue = getgenv().ballMagnetActive,
    Callback = function(v)
        getgenv().ballMagnetActive = v
        StarterGui:SetCore("SendNotification", {
            Title = "Ball Magnet",
            Text = getgenv().ballMagnetActive and "Enabled." or "Disabled.",
            Duration = 1
        })
        local remote = ReplicatedStorage:WaitForChild("ByteNetReliable")
        if getgenv().ballMagnetActive then
            if getgenv().ballMagnetConnection then getgenv().ballMagnetConnection:Disconnect() end
            getgenv().ballMagnetConnection = task.spawn(function()
                while getgenv().ballMagnetActive do
                    pcall(function() remote:FireServer(buffer.fromstring("\033")) end)
                    task.wait()
                end
            end)
        else
            if getgenv().ballMagnetConnection then getgenv().ballMagnetConnection = nil end
        end
    end
})

do
    local magnetEnabled = false
    local beganConnection, endedConnection, holding = nil, nil, false
    silent:CreateToggle({
        Name = "Ball Magnet (Bind: M2 Hold)",
        Description = "Hold right-click to magnet the ball.",
        CurrentValue = false,
        Callback = function()
            magnetEnabled = not magnetEnabled
            if magnetEnabled then
                beganConnection = UserInputService.InputBegan:Connect(function(input, gameProcessed)
                    if gameProcessed then return end
                    if input.UserInputType == Enum.UserInputType.MouseButton2 then
                        holding = true
                        task.spawn(function()
                            local remote = ReplicatedStorage:WaitForChild("ByteNetReliable")
                            while holding do
                                remote:FireServer(buffer.fromstring("\033"))
                                task.wait(0.05)
                            end
                        end)
                    end
                end)
                endedConnection = UserInputService.InputEnded:Connect(function(input, gameProcessed)
                    if gameProcessed then return end
                    if input.UserInputType == Enum.UserInputType.MouseButton2 then holding = false end
                end)
                StarterGui:SetCore("SendNotification", { Title = "Ball Magnet (M2 Hold)", Text = "Enabled.", Duration = 3 })
            else
                holding = false
                if beganConnection then beganConnection:Disconnect() beganConnection = nil end
                if endedConnection then endedConnection:Disconnect() endedConnection = nil end
                StarterGui:SetCore("SendNotification", { Title = "Ball Magnet (M2 Hold)", Text = "Disabled.", Duration = 3 })
            end
        end
    })
end

getgenv().ballMagnetRadius = getgenv().ballMagnetRadius or 1
silent:CreateSlider({
    Name = "Ball Magnet Radius",
    Range = {1,50},
    Increment = 0.1,
    CurrentValue = getgenv().ballMagnetRadius,
    Callback = function(v) getgenv().ballMagnetRadius = v end
})
if getgenv().ballMagnetConnection then getgenv().ballMagnetConnection:Disconnect() end
do
    local lastFire = 0
    local FIRE_INTERVAL = 0.1
    local hrp
    local localPlayer = Players.LocalPlayer
    local function setupCharacter(char) hrp = char:WaitForChild("HumanoidRootPart", 10) end
    if localPlayer.Character then setupCharacter(localPlayer.Character) end
    localPlayer.CharacterAdded:Connect(setupCharacter)
    getgenv().ballMagnetConnection = RunService.Heartbeat:Connect(function()
        local radius = getgenv().ballMagnetRadius or 1
        if radius < 2 then return end
        local ball = Workspace.Terrain:FindFirstChild("Ball")
        if not ball then return end
        if hrp then
            local dist = (hrp.Position - ball.Position).Magnitude
            if dist <= radius and (time() - lastFire) >= FIRE_INTERVAL then
                lastFire = time()
                pcall(function() ReplicatedStorage:WaitForChild("ByteNetReliable"):FireServer(buffer.fromstring("\033")) end)
            end
        end
    end)
end

silent:CreateSection("Auto Counters")
silent:CreateLabel({ Text = "Notification Suppression: Useful When Streaming.", Style = 2 })
silent:CreateToggle({ Name = "Suppress Notifications", Description = "Toggle notification suppression.", CurrentValue = _G.suppressNotifs, Callback = function(v) _G.suppressNotifs = v StarterGui:SetCore("SendNotification", { Title = "Notifications", Text = _G.suppressNotifs and "Suppressed." or "Un-Suppressed.", Duration = 1 }) end })

_G.autoSkills = _G.autoSkills or { ["toggleDetection"]="dribble", ["toggleCounter1"]="skill1", ["toggleCounter2"]="skill2", ["toggleCounter3"]="skill3", ["toggleCounter4"]="skill4", ["toggleCounter5"]="skill5" }

do
    local player = Players.LocalPlayer
    local ANIMATION_ID = "rbxassetid://109744655458082"
    local cooldowns = {}
    RunService.Stepped:Connect(function()
        local char = player.Character
        local root = char and char:FindFirstChild("HumanoidRootPart")
        if not root then return end
        for _, target in ipairs(Players:GetPlayers()) do
            if target ~= player and target.Character and target.Team ~= player.Team then
                local targetHRP = target.Character:FindFirstChild("HumanoidRootPart")
                local targetHumanoid = target.Character:FindFirstChild("Humanoid")
                local animator = targetHumanoid and targetHumanoid:FindFirstChildOfClass("Animator")
                if targetHRP and animator then
                    local distance = (root.Position - targetHRP.Position).Magnitude
                    if distance <= 35 then
                        local now = tick()
                        local last = cooldowns[target] or 0
                        if now - last >= 0.5 then
                            for _, track in ipairs(animator:GetPlayingAnimationTracks()) do
                                if track.Animation.AnimationId == ANIMATION_ID then
                                    cooldowns[target] = now
                                    for _, toggleKey in ipairs({ "toggleDetection", "toggleCounter1", "toggleCounter2", "toggleCounter3", "toggleCounter4", "toggleCounter5" }) do
                                        local skillName = _G.autoSkills[toggleKey]
                                        if _G[toggleKey] and skillName then
                                            local args
                                            if skillName == "dribble" then
                                                args = { buffer.fromstring("\034\001"), { { skillName, false } } }
                                            else
                                                args = { buffer.fromstring("\034\001"), { { skillName } } }
                                            end
                                            ReplicatedStorage:WaitForChild("ByteNetReliable"):FireServer(unpack(args))
                                        end
                                    end
                                    break
                                end
                            end
                        end
                    end
                end
            end
        end
    end)
end

silent:CreateToggle({ Name = "Toggle Keybinds", Description = "Enable keybinds (Z/X/C/V/N/M)", CurrentValue = (_G.keybindsEnabled or false), Callback = function(v) _G.keybindsEnabled = v notify("Keybinds", v and "Enabled." or "Disabled.", 3) end })

local function createAutoCounterUI(name, key)
    silent:CreateToggle({ Name = name, Description = "Auto counter toggle", CurrentValue = _G[key] or false, Callback = function(v) _G[key] = v notify(name, v and "Enabled." or "Disabled.", 3) end })
end

createAutoCounterUI("Auto Dribble", "toggleDetection")
createAutoCounterUI("Auto Counter (Move One)", "toggleCounter1")
createAutoCounterUI("Auto Counter (Move Two)", "toggleCounter2")
createAutoCounterUI("Auto Counter (Move Three)", "toggleCounter3")
createAutoCounterUI("Auto Counter (Move Four)", "toggleCounter4")
createAutoCounterUI("Auto Counter (Move Five)", "toggleCounter5")

if not _G._keybindSetup then
    _G._keybindSetup = true
    UserInputService.InputBegan:Connect(function(input, gameProcessed)
        if gameProcessed or not _G.keybindsEnabled then return end
        local keyMap = {
            [Enum.KeyCode.Z] = {"toggleDetection", "Auto-Dribble"},
            [Enum.KeyCode.X] = {"toggleCounter1", "Auto-Counter (Move One)"},
            [Enum.KeyCode.C] = {"toggleCounter2", "Auto-Counter (Move Two)"},
            [Enum.KeyCode.V] = {"toggleCounter3", "Auto-Counter (Move Three)"},
            [Enum.KeyCode.N] = {"toggleCounter4", "Auto-Counter (Move Four)"},
            [Enum.KeyCode.M] = {"toggleCounter5", "Auto-Counter (Move Five)"}
        }
        local mapping = keyMap[input.KeyCode]
        if mapping then
            local toggleKey, label = unpack(mapping)
            _G[toggleKey] = not (_G[toggleKey] or false)
            notify(label, _G[toggleKey] and "Enabled." or "Disabled.", 3)
        end
    end)
end

blatant:CreateSection("Auto Goal Features")
blatant:CreateButton({ Name = "Barou Devour Goal", Description = "Teleport sequence using two enemies", Callback = function()
    local plr = Players.LocalPlayer
    local function countEnimTeamMembers()
        local op = game.Teams:FindFirstChild("A")
        if plr.Team == game.Teams:FindFirstChild("A") then op = game.Teams:FindFirstChild("B") end
        local count = 0
        for _, player in ipairs(Players:GetPlayers()) do if player.Team == op then count += 1 end end
        return count
    end
    local function getTwoRandomEnemies()
        local enemies = {}
        local op = game.Teams:FindFirstChild("A")
        if plr.Team == game.Teams:FindFirstChild("A") then op = game.Teams:FindFirstChild("B") end
        for _, player in ipairs(Players:GetPlayers()) do if player ~= plr and player.Team == op then table.insert(enemies, player) end end
        for i = #enemies, 2, -1 do local j = math.random(1,i); enemies[i], enemies[j] = enemies[j], enemies[i] end
        return enemies[1], enemies[2]
    end
    if countEnimTeamMembers() >= 2 then
        local p1, p2 = getTwoRandomEnemies()
        if not p1 or not p2 then return end
        local function tpToPlayer(targetPlayer)
            local char = plr.Character or plr.CharacterAdded:Wait()
            local root = char:WaitForChild("HumanoidRootPart")
            local targetRoot = targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart")
            if root and targetRoot then root.CFrame = targetRoot.CFrame end
        end
        tpToPlayer(p1); task.wait(0.3); tpToPlayer(p2); task.wait(0.3)
        if plr.Team == game.Teams:FindFirstChild("A") then plr.Character.HumanoidRootPart.Position = Vector3.new(-625,3,925)
        elseif plr.Team == game.Teams:FindFirstChild("B") then plr.Character.HumanoidRootPart.Position = Vector3.new(-588,3,1643) end
    end
end })

blatant:CreateButton({ Name = "Nagi Dream Goal", Description = "Perform Nagi Dream Goal", Callback = function()
    local plr = Players.LocalPlayer
    if plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
        local hrp = plr.Character.HumanoidRootPart
        if plr.Team == game.Teams:FindFirstChild("A") then
            hrp.CFrame = CFrame.new(-459, 3, 862)
        elseif plr.Team == game.Teams:FindFirstChild("B") then
            hrp.CFrame = CFrame.new(-611, 3, 1682) * CFrame.Angles(0, math.rad(180), 0)
        end
        if plr.Character and plr.Character:FindFirstChild("Humanoid") then plr.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping) end
        task.wait(0.2)
        VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.Two, false, game)
        VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.Two, false, game)
    end
end })

blatant:CreateButton({ Name = "Shidou Back-Heel Goal (Must Be Overtime.)", Description = "Back-heel goal (overtime)", Callback = function()
    local plr = Players.LocalPlayer
    if plr.Team == game.Teams:FindFirstChild("A") then plr.Character.HumanoidRootPart.Position = Vector3.new(-625,3,925)
    elseif plr.Team == game.Teams:FindFirstChild("B") then plr.Character.HumanoidRootPart.Position = Vector3.new(-588,3,1643) end
    task.wait(0.25)
    VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.Two, false, game)
    VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.Two, false, game)
end })

blatant:CreateToggle({ Name = "Blatant Mode", Description = "Disable collisions on certain map parts", CurrentValue = false, Callback = function(v)
    local blatantEnabled = v
    local loopConnection
    if blatantEnabled then
        loopConnection = RunService.Heartbeat:Connect(function()
            local ok, map = pcall(function() return Workspace.map end)
            if ok and map then
                local a = map:FindFirstChild("gkbarriar") and map.gkbarriar:FindFirstChild("Abarriar")
                local b = map:FindFirstChild("gkbarriar") and map.gkbarriar:FindFirstChild("Bbarriar")
                local ag = map:FindFirstChild("Agoal")
                local bg = map:FindFirstChild("Bgoal")
                if a then a.CanCollide = false end
                if b then b.CanCollide = false end
                if ag then ag.CanCollide = false end
                if bg then bg.CanCollide = false end
            end
        end)
        StarterGui:SetCore("SendNotification", { Title="Blatant Mode", Text="Enabled.", Duration=3 })
    else
        if loopConnection then loopConnection:Disconnect() loopConnection = nil end
        local ok, map = pcall(function() return Workspace.map end)
        if ok and map then
            if map:FindFirstChild("gkbarriar") then
                local a = map.gkbarriar:FindFirstChild("Abarriar")
                local b = map.gkbarriar:FindFirstChild("Bbarriar")
                if a then a.CanCollide = true end
                if b then b.CanCollide = true end
            end
            if map:FindFirstChild("Agoal") then map.Agoal.CanCollide = true end
            if map:FindFirstChild("Bgoal") then map.Bgoal.CanCollide = true end
        end
        StarterGui:SetCore("SendNotification", { Title="Blatant Mode", Text="Disabled.", Duration=3 })
    end
end })

blatant:CreateToggle({ Name = "Spam Brick Sound (6V6 + GK Only.)", Description = "Spam to kick to target position", CurrentValue = false, Callback = function(state)
    local running = state
    if running then
        StarterGui:SetCore("SendNotification", { Title = "Brick Spam", Text = "Enabled.", Duration = 3 })
        task.spawn(function()
            local char = Players.LocalPlayer.Character
            local root = char and char:WaitForChild("HumanoidRootPart")
            local team = Players.LocalPlayer.Team and Players.LocalPlayer.Team.Name
            local targetPos
            if team == "A" then targetPos = Vector3.new(-510,3,1706) elseif team == "B" then targetPos = Vector3.new(-559,3,843) end
            if targetPos then
                while running do
                    if root then root.CFrame = CFrame.new(targetPos) end
                    wait(0.1)
                    local args = { buffer.fromstring("\034\001"), { { "kick", 27, false, vector.create(0,1,0) } } }
                    ReplicatedStorage:WaitForChild("ByteNetReliable"):FireServer(unpack(args))
                    task.wait(0.05)
                end
            end
        end)
    else
        StarterGui:SetCore("SendNotification", { Title = "Brick Spam", Text = "Disabled.", Duration = 3 })
    end
end })

blatant:CreateButton({ Name = "Manual Score", Description = "Teleport to opponent goal to score manually", Callback = function()
    if Players.LocalPlayer.Character and Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
        if Workspace:FindFirstChild("map") and Workspace.map:FindFirstChild("Agoal") and Workspace.map:FindFirstChild("Bgoal") then
            if Players.LocalPlayer.Team then
                Players.LocalPlayer.Character.HumanoidRootPart.CFrame =
                    (Players.LocalPlayer.Team.Name == "A" and Workspace.map.Bgoal or Workspace.map.Agoal).CFrame
            end
        end
    end
end })

