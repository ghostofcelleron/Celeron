local airDribbleEnabled = false
silent:CreateToggle({ Name = "Air Dribble (Bind: LeftAlt)", Description = "Perform air dribble using LeftAlt", CurrentValue = airDribbleEnabled, Callback = function(v) airDribbleEnabled = v StarterGui:SetCore("SendNotification", { Title = "Air Dribble", Text = airDribbleEnabled and "Enabled." or "Disabled.", Duration = 2 }) end })
