    local parts = string.split(fn,".")
    local ok = (#parts==1 and getfenv()[fn]) or (#parts==2 and getfenv()[parts[1]] and getfenv()[parts[1]][parts[2]])
    if not ok then
        notify("executor not supported.")
        return
    end
end

