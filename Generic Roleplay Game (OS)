local Players = game:GetService("Players")
local StarterGui = game:GetService("StarterGui")
local player = Players.LocalPlayer

local function startCheck()
    local function checkSuspicious()
        local val = player:GetAttribute("IsSuspicious")
        if val == true then
            StarterGui:SetCore("SendNotification", {
                Title = "Alert",
                Text = "You are currently marked as suspicious. Please switch servers to fix.",
                Duration = 5
            })
        end
        task.delay(5, checkSuspicious)
    end
    checkSuspicious()
end

player.CharacterAdded:Connect(function()
    startCheck()
end)

if player.Character then
    startCheck()
end
