local Players = game:GetService("Players")

function teleportPlayer(targetPosition)
    local player = Players.LocalPlayer
    if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        local hrp = player.Character.HumanoidRootPart
        local startPos = hrp.Position
        local direction = (targetPosition - startPos).Unit
        local distance = (targetPosition - startPos).Magnitude
        local steps = math.ceil(distance / 20)

        for i = 1, steps do
            local newPos = startPos + direction * math.min(i * 20, distance)
            hrp.CFrame = CFrame.new(newPos, targetPosition)
            task.wait()
        end
    end
end

