local ui = loadstring(game:HttpGet("https://raw.githubusercontent.com/Daffy734/code/refs/heads/main/yep"))()

local win = ui:Create({
    Name = "Celeron's GUI (Life in Prison)",
    ThemeColor = Color3.fromRGB(100, 100, 100),
    StartupSound = "rbxassetid://6958727243",
    ThemeFont = Enum.Font.FredokaOne
})

local function withTeleportBack(callback)
    return function()
        local p = game.Players.LocalPlayer
        local char = p.Character or p.CharacterAdded:Wait()
        local hrp = char:WaitForChild("HumanoidRootPart")
        local originalCFrame = hrp.CFrame

        callback()

        task.delay(0.6, function()
            if hrp and hrp.Parent then
                hrp.CFrame = originalCFrame
            end
        end)
    end
end

local maintab = win:Tab("Main")
local itemsTab = win:Tab("Items")
local teleportTab = win:Tab("Teleports")
local misctab = win:Tab("Others")
local helptab = win:Tab("Info")

maintab:Label("Script Made By Celeron + Daffy!")
itemsTab:Label("Manually Grab The Gun, And You Will Be Teleported Back!")
teleportTab:Label("Teleport Features.")
misctab:Label("Other Features.")
helptab:Label("GUI Controls Are Below, Script Credits Are At The Bottom!")
helptab:Label("Show / Hide GUI: Right Alt")

local players = game:GetService("Players")
local localPlayer = players.LocalPlayer

local targetSpeed = 18
local targetJump = 50
local humanoid

local function applyStats()
    if humanoid then
        humanoid.WalkSpeed = targetSpeed
        humanoid.UseJumpPower = true
        humanoid.JumpPower = targetJump
    end
end

local function setupCharacter(char)
    humanoid = char:WaitForChild("Humanoid")
    applyStats()
end

if localPlayer.Character then
    setupCharacter(localPlayer.Character)
end

localPlayer.CharacterAdded:Connect(setupCharacter)

maintab:Slider("Player Speed", targetSpeed, 5, 250, 1, function(v)
    targetSpeed = v
    applyStats()
end)

maintab:Slider("Player Jump-Power", targetJump, 25, 250, 1, function(v)
    targetJump = v
    applyStats()
end)

local flightEnabled = false
local flightLoop = nil
local bg = nil
local bv = nil

maintab:Toggle("Flight", function()
    flightEnabled = not flightEnabled
    
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "Flight",
        Text = flightEnabled and "Enabled." or "Disabled.",
        Duration = 2
    })

    if flightLoop then
        flightLoop:Disconnect()
        flightLoop = nil
    end

    local player = game:GetService("Players").LocalPlayer
    local uis = game:GetService("UserInputService")
    local cam = workspace.CurrentCamera
    local char = player.Character or player.CharacterAdded:Wait()
    local hrp = char:WaitForChild("HumanoidRootPart")
    local hum = char:WaitForChild("Humanoid")

    if bg then bg:Destroy() bg = nil end
    if bv then bv:Destroy() bv = nil end

    if flightEnabled then
        hum.AutoRotate = false
        hum.PlatformStand = true

        for _, part in ipairs(char:GetDescendants()) do
            if part:IsA("BasePart") then
                part.CanCollide = false
            end
        end

        bg = Instance.new("BodyGyro")
        bg.MaxTorque = Vector3.new(1e6, 1e6, 1e6)
        bg.P = 1e5
        bg.CFrame = hrp.CFrame
        bg.Parent = hrp

        bv = Instance.new("BodyVelocity")
        bv.MaxForce = Vector3.new(1e9, 1e9, 1e9)
        bv.Velocity = Vector3.zero
        bv.Parent = hrp

        local currentSpeed = 1.35

        flightLoop = game:GetService("RunService").RenderStepped:Connect(function()
            bv.Velocity = Vector3.zero

            local moveDir = Vector3.zero
            if uis:IsKeyDown(Enum.KeyCode.W) then moveDir += cam.CFrame.LookVector end
            if uis:IsKeyDown(Enum.KeyCode.S) then moveDir -= cam.CFrame.LookVector end
            if uis:IsKeyDown(Enum.KeyCode.A) then moveDir -= cam.CFrame.RightVector end
            if uis:IsKeyDown(Enum.KeyCode.D) then moveDir += cam.CFrame.RightVector end

            if moveDir.Magnitude > 0 then
                hrp.CFrame = hrp.CFrame + moveDir.Unit * currentSpeed
            end

            local look = cam.CFrame.LookVector
            local up = cam.CFrame.UpVector
            local target = CFrame.lookAt(hrp.Position, hrp.Position + look, up)
            bg.CFrame = bg.CFrame:Lerp(target, 0.35)
        end)
    else
        hum.PlatformStand = false
        hum.AutoRotate = true

        for _, part in ipairs(char:GetDescendants()) do
            if part:IsA("BasePart") then
                part.CanCollide = true
            end
        end

        if bg then bg:Destroy() bg = nil end
        if bv then bv:Destroy() bv = nil end
    end
end)

local aimlockEnabled = false
local aimlockTarget = nil
local fovCircle = nil
local uis = game:GetService("UserInputService")
local rs = game:GetService("RunService")
local plr = game:GetService("Players").LocalPlayer
local cam = workspace.CurrentCamera
local bindConnection = nil
local renderConnection = nil

maintab:Toggle("Aimlock (Bind: M2)", function()
    aimlockEnabled = not aimlockEnabled
    if aimlockEnabled then
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "Aimlock",
            Text = "Enabled.",
            Duration = 2
        })
        fovCircle = Drawing.new("Circle")
        fovCircle.Visible = true
        fovCircle.Color = Color3.fromRGB(255,255,255)
        fovCircle.Thickness = 1
        fovCircle.NumSides = 64
        fovCircle.Radius = 128
        fovCircle.Filled = false
        fovCircle.Position = Vector2.new(cam.ViewportSize.X/2, cam.ViewportSize.Y/2)

        bindConnection = uis.InputBegan:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton2 then
                if aimlockTarget then
                    aimlockTarget = nil
                else
                    local closest, closestDist = nil, math.huge
                    local screenCenter = Vector2.new(cam.ViewportSize.X/2, cam.ViewportSize.Y/2)
                    for _,v in pairs(game:GetService("Players"):GetPlayers()) do
                        if v ~= plr and v.Team ~= plr.Team and v.Character and v.Character:FindFirstChild("HumanoidRootPart") then
                            local pos, onScreen = cam:WorldToViewportPoint(v.Character.HumanoidRootPart.Position)
                            if onScreen then
                                local dist = (Vector2.new(pos.X,pos.Y) - screenCenter).Magnitude
                                if dist <= fovCircle.Radius and dist < closestDist then
                                    closestDist = dist
                                    closest = v
                                end
                            end
                        end
                    end
                    aimlockTarget = closest
                end
            end
        end)

        renderConnection = rs.RenderStepped:Connect(function()
            if aimlockEnabled and fovCircle then
                fovCircle.Position = Vector2.new(cam.ViewportSize.X/2, cam.ViewportSize.Y/2)
                if aimlockTarget and aimlockTarget.Character and aimlockTarget.Character:FindFirstChild("HumanoidRootPart") then
                    cam.CFrame = CFrame.new(cam.CFrame.Position, aimlockTarget.Character.HumanoidRootPart.Position)
                end
            end
        end)
    else
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "Aimlock",
            Text = "Disabled.",
            Duration = 2
        })
        if fovCircle then
            fovCircle:Remove()
            fovCircle = nil
        end
        if bindConnection then
            bindConnection:Disconnect()
            bindConnection = nil
        end
        if renderConnection then
            renderConnection:Disconnect()
            renderConnection = nil
        end
        aimlockTarget = nil
    end
end)

local espEnabled = false
local espObjects = {}
local rs = game:GetService("RunService")
local plr = game:GetService("Players").LocalPlayer
local cam = workspace.CurrentCamera
local renderConnection = nil

local function createESP(v)
    local box = Drawing.new("Square")
    box.Thickness = 1
    box.Filled = false

    local healthBG = Drawing.new("Square")
    healthBG.Filled = true
    healthBG.Color = Color3.fromRGB(0,0,0)

    local healthBar = Drawing.new("Square")
    healthBar.Filled = true

    local nameTag = Drawing.new("Text")
    nameTag.Size = 16
    nameTag.Center = true

    espObjects[v] = {box=box,healthBG=healthBG,healthBar=healthBar,nameTag=nameTag}
end

local function removeESP(v)
    if espObjects[v] then
        for _,obj in pairs(espObjects[v]) do
            obj:Remove()
        end
        espObjects[v] = nil
    end
end

maintab:Toggle("Player ESP", function()
    espEnabled = not espEnabled
    if espEnabled then
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "Player ESP",
            Text = "Enabled.",
            Duration = 2
        })

        for _,v in pairs(game:GetService("Players"):GetPlayers()) do
            if v ~= plr then
                createESP(v)
                v.CharacterAdded:Connect(function()
                    removeESP(v)
                    createESP(v)
                end)
            end
        end

        renderConnection = rs.RenderStepped:Connect(function()
            for v,objs in pairs(espObjects) do
                local char = v.Character
                if espEnabled and char and char:FindFirstChild("HumanoidRootPart") and char:FindFirstChild("Humanoid") then
                    local hrp = char.HumanoidRootPart
                    local hum = char.Humanoid
                    local pos,onScreen = cam:WorldToViewportPoint(hrp.Position)
                    if onScreen then
                        local boxSize = Vector2.new(50,75)
                        local boxPos = Vector2.new(pos.X - boxSize.X/2, pos.Y - boxSize.Y/2)

                        objs.box.Visible = true
                        objs.box.Color = v.Team and v.Team.TeamColor.Color or Color3.fromRGB(255,255,255)
                        objs.box.Size = boxSize
                        objs.box.Position = boxPos

                        objs.healthBG.Visible = true
                        objs.healthBG.Size = Vector2.new(4, boxSize.Y)
                        objs.healthBG.Position = Vector2.new(boxPos.X - 6, boxPos.Y)

                        local hpPercent = math.clamp(hum.Health/hum.MaxHealth,0,1)
                        objs.healthBar.Visible = true
                        objs.healthBar.Color = Color3.fromRGB(0,255,0)
                        objs.healthBar.Size = Vector2.new(4, boxSize.Y*hpPercent)
                        objs.healthBar.Position = Vector2.new(boxPos.X - 6, boxPos.Y + (boxSize.Y - boxSize.Y*hpPercent))

                        objs.nameTag.Visible = true
                        objs.nameTag.Text = v.Name
                        objs.nameTag.Color = Color3.fromRGB(255,255,255)
                        objs.nameTag.Position = Vector2.new(pos.X, boxPos.Y - 16)
                    else
                        objs.box.Visible = false
                        objs.healthBG.Visible = false
                        objs.healthBar.Visible = false
                        objs.nameTag.Visible = false
                    end
                else
                    objs.box.Visible = false
                    objs.healthBG.Visible = false
                    objs.healthBar.Visible = false
                    objs.nameTag.Visible = false
                end
            end
        end)
    else
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "Player ESP",
            Text = "Disabled.",
            Duration = 2
        })
        if renderConnection then
            renderConnection:Disconnect()
            renderConnection = nil
        end
        for v,objs in pairs(espObjects) do
            for _,obj in pairs(objs) do
                obj:Remove()
            end
        end
        espObjects = {}
    end
end)

misctab:Button("Infinite Yield", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
end)

local Players = game:GetService("Players")
local player = Players.LocalPlayer

local function teleportTo(x, y, z)
    local character = player.Character or player.CharacterAdded:Wait()
    local hrp = character:WaitForChild("HumanoidRootPart")
    hrp.CFrame = CFrame.new(x, y, z)
end

local function grabItemTeleport(x, y, z, itemName)
    local character = player.Character or player.CharacterAdded:Wait()
    local hrp = character:WaitForChild("HumanoidRootPart")
    local originalCFrame = hrp.CFrame
    teleportTo(x, y, z)
    repeat task.wait(0.1) until player.Backpack:FindFirstChild(itemName)
    hrp.CFrame = originalCFrame
end

