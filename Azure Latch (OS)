maintab:Label("Ball Features.")
maintab:Toggle("Auto Goal", function()
    _G.AUTO_GOAL = not _G.AUTO_GOAL
    if _G.AUTO_GOAL then
        game.StarterGui:SetCore("SendNotification", {
