maintab:Label("Ball Features.")
maintab:Toggle("Auto Goal", "Attempts To Steal The Ball And Score.", function()
    _G.AUTO_GOAL = not _G.AUTO_GOAL
    if _G.AUTO_GOAL then
        game.StarterGui:SetCore("SendNotification", {
            Title = "Auto Goal",
            Text = "Enabled.",
            Duration = 1
